{
    "manifest_version": 2,
    "name": "lueyNote",
    "version": "1.0",
    "description": "luey note",
    "browser_action": {
        "default_popup": "index.html"
    },
    "commands": {
        "select_panel": {
            "description": "打开收藏页",
            "global": true,
            "suggested_key": {
                "default": "Ctrl+Shift+A"
            }
        }
    },
    "background": {
        "scripts": [
            "js/chunk-vendors.js",
            "js/chunk-common.js",
            "js/aboutus.js"
        ],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": [
                "\u003Call_urls>"
            ],
            "run_at": "document_end",
            "js": [
                "js/chunk-vendors.js",
                "js/chunk-common.js",
                "js/helpCenter.js"
            ]
        }
    ],
    "permissions": [
        "\u003Call_urls>",
        "clipboard",
        "clipboardRead",
        "clipboardWrite",
        "storage",
        "activeTab",
        "scripting",
        "*://*/*",
        "contextMenus"
    ],
    "web_accessible_resources": [
        "*"
    ]
}